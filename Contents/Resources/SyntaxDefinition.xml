<?xml version="1.0" encoding="UTF-8"?>
<syntax>
    <head>
        <name>Markdown</name>
        <autocompleteoptions use-spelling-dictionary="yes"/>
        <charsintokens><![CDATA[_0987654321abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ@]]></charsintokens>
    </head>
    <states>
        <default id="Base" color="#000000" inverted-color="#FFFFFF" usespellchecking="yes">
            <!-- Headers -->
            <keywords id="Header" color="#CC0000" inverted-color="#cc66ff" font-weight="bold">
                <regex>^#{1,6}\s*[^#\r\n]*#{0,6}$</regex>
            </keywords>
        </default>
    </states>
</syntax>